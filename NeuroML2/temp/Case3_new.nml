<neuroml xmlns="http://www.neuroml.org/schema/neuroml2"
  xmlns:meta="http://morphml.org/metadata/schema"
  xmlns:mml="http://morphml.org/morphml/schema"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
  xsi:schemaLocation="http://www.neuroml.org/schema/neuroml2  https://raw.github.com/NeuroML/NeuroML2/master/Schemas/NeuroML2/NeuroML_v2beta.xsd"
  id="Case3_new">

  <cell id="Case3_new">
      <notes>
        Neuronal morphology exported in NeuroML v2beta from CVapp (NeuroMorpho.org version)
        Original file: Case3_new.swc </notes>

      <morphology id="morphology_Case3_new">

        <!-- First point is of a multi point soma => not spherical! -->

        <segment id="0" name="Seg_0"> <!-- "Cable" is 0-->
          
          <!-- Cylindrical soma... -->
          <proximal x="0" y="-10" z="0" diameter="20"/>
          <distal x="0" y="0" z="0" diameter="20"/>
        </segment>  

        <segment id="1" name="Seg_1"> <!-- "Cable" is 0-->
          <parent segment="0"/>
          <distal x="0" y="10" z="0" diameter="20"/>
        </segment>  

        <!-- Parent point is on soma! Not creating 'real' segment -->

        <segment id="2" name="Seg_2"> <!-- "Cable" is 1-->
          <parent segment="0"/>
          <proximal x="10" y="0" z="0" diameter="4"/>
          <distal x="20" y="0" z="0" diameter="4"/>
        </segment>  

        <segment id="3" name="Seg_3"> <!-- "Cable" is 1-->
          <parent segment="2"/>
          <distal x="30" y="0" z="0" diameter="4"/>
        </segment>  

        <segmentGroup id="Cable_0" >
          <member segment="0"/>
          <member segment="1"/>
        </segmentGroup>

        <segmentGroup id="Cable_1" >
          <member segment="2"/>
          <member segment="3"/>
        </segmentGroup>

        <segmentGroup id="dendrite_group" >
          <include segmentGroup="Cable_1"/>
        </segmentGroup>

        <segmentGroup id="soma_group" >
          <include segmentGroup="Cable_0"/>
        </segmentGroup>

        <segmentGroup id="color_green" >
          <include segmentGroup="Cable_1"/>
        </segmentGroup>

        <segmentGroup id="basal_dendrite" >
          <include segmentGroup="Cable_1"/>
        </segmentGroup>

        <segmentGroup id="all" >
          <include segmentGroup="Cable_0"/>
          <include segmentGroup="Cable_1"/>
        </segmentGroup>

        <segmentGroup id="color_white" >
          <include segmentGroup="Cable_0"/>
        </segmentGroup>

      </morphology>

<!--NOTE: TEMPORARILY adding default biophysical parameters to test visualisation of NML2 cells on OSB-->
<biophysicalProperties id="biophys">
<membraneProperties>
<channelDensity condDensity="0.02812149 mS_per_cm2" id="pas_all" ionChannel="pas" ion="non_specific" erev="-66.0 mV"/>
<spikeThresh value="0 mV"/>
<specificCapacitance value="1.57 uF_per_cm2"/>
<initMembPotential value="-60.0 mV"/>
</membraneProperties>
<intracellularProperties>
<resistivity value="0.3 kohm_cm"/>
</intracellularProperties>
</biophysicalProperties>

  </cell>
</neuroml>
